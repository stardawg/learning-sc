doctype html
html(lang="en")
	head
		title= title
		link(rel="stylesheet",href='/css/bootstrap.min.css')
	body

		div.loading-page
			img(src='/images/preloader.gif', alt='preloader image')

		div.whole-page

			include ./navbar

			div.container-fluid
				div.row
					div.col-md-12(style="margin-top:50px;")

						section(data-route='dashboard', style="display:none;")
							include dashboard
						section(data-route="about",style="display:none;")
							include about
						section(data-route="users",style="display:none;")
							include users


script(src='/js/socketcluster.js')

script(src='/js/libraries/jquery.min.js')
script(src='/js/libraries/bootstrap.min.js')
